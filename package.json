{
  "name": "mira-booking",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:deploy": "SKIP_TYPESCRIPT_CHECK=1 next build",
    "build:cloudflare": "cross-env NEXT_TELEMETRY_DISABLED=1 next build --no-lint",
    "build:no-ts": "node build-no-ts.js",
    "start": "next start",
    "lint": "next lint",
    "deploy": "npx wrangler deploy",
    "deploy:watch": "node deploy-realtime.js",
    "deploy:instant": "CF_DEPLOY_WAIT=0 npm run deploy",
    "check:token": "node check-api-token.js",
    "deploy:manual": "npx wrangler deploy --env=production",
    "generate:token": "npx wrangler --version && echo \"To create your API token, go to https://dash.cloudflare.com/profile/api-tokens and use the 'Edit Cloudflare Workers' template\""
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@prisma/client": "^6.6.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.16",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "formidable": "^3.5.4",
    "hono": "^4.7.7",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.503.0",
    "mrz-detection": "^0.4.1",
    "next": "^14.0.4",
    "postcss": "^8.4.31",
    "prisma": "^6.6.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.21.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^7.5.2",
    "tailwindcss": "^3.3.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250426.0",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-datepicker": "^4.19.3",
    "@types/react-dom": "^18.2.15",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.2",
    "typescript": "^5.2.2",
    "wrangler": "^4.12.1"
  }
}
